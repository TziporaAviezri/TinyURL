    "outDir": "./dist",
    "esModuleInterop": true,
    "strict": true,
    "baseUrl": "src",
    "paths": {
      "@app/*": ["/*"],
      "@TinyURL/*": ["TinyURL/*"],
      "@APIQuickConfiguration/*":["API-quick-configuration/*"]
    }
  },
  "exclude": ["./node_modules"]
}
